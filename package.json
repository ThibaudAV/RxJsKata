{
  "name": "rx-js-kata",
  "title": "Kata for RxJS",
  "description": "A kata on RxJS that should get you up to speed with test schedulers",
  "version": "0.0.1",
  "maintainers": [
    {
      "name": "Alexandre Victoor",
      "web": "https://github.com/alexvictoor"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexvictoor/RxJsKata.git"
  },
  "bugs": {
    "url": "https://github.com/alexvictoor/RxJsKata/issues"
  },
  "licenses": [
    {
      "type": "Apache License, Version 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "scripts": {
    "build": "tsc",
    "prettier": "prettier --write '**/*.{ts,js,md,json}'",
    "test": "mocha -r ts-node/register --ui mocha-qunit-ui -b"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "mocha": "*",
    "mocha-qunit-ui": "*",
    "rx": "*"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.4",
    "prettier": "1.16.4",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  }
}
